<keymap version="1" name="Windows copy" parent="$default">
  <action id="$Undo">
    <keyboard-shortcut first-keystroke="ctrl z" />
    <keyboard-shortcut first-keystroke="alt back_space" />
    <keyboard-shortcut first-keystroke="alt z" />
  </action>
  <action id="ActivateCommitToolWindow" />
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt f12" />
    <keyboard-shortcut first-keystroke="alt 0" />
  </action>
  <action id="EditorCompleteStatement">
    <keyboard-shortcut first-keystroke="shift ctrl enter" />
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="EditorDown">
    <keyboard-shortcut first-keystroke="down" />
    <keyboard-shortcut first-keystroke="alt j" />
  </action>
  <action id="EditorLeft">
    <keyboard-shortcut first-keystroke="left" />
    <keyboard-shortcut first-keystroke="alt h" />
  </action>
  <action id="EditorLineEnd">
    <keyboard-shortcut first-keystroke="end" />
    <keyboard-shortcut first-keystroke="alt e" />
  </action>
  <action id="EditorLineStart">
    <keyboard-shortcut first-keystroke="home" />
    <keyboard-shortcut first-keystroke="alt a" />
  </action>
  <action id="EditorRight">
    <keyboard-shortcut first-keystroke="right" />
    <keyboard-shortcut first-keystroke="alt l" />
  </action>
  <action id="EditorUp">
    <keyboard-shortcut first-keystroke="up" />
    <keyboard-shortcut first-keystroke="alt k" />
  </action>
  <action id="Generate">
    <keyboard-shortcut first-keystroke="alt insert" />
    <keyboard-shortcut first-keystroke="alt n" />
  </action>
  <action id="GotoAction">
    <keyboard-shortcut first-keystroke="shift ctrl a" />
    <keyboard-shortcut first-keystroke="shift alt p" />
  </action>
  <action id="NewElement">
    <keyboard-shortcut first-keystroke="alt insert" />
    <keyboard-shortcut first-keystroke="alt n" />
  </action>
  <action id="编辑Undo">
    <keyboard-shortcut first-keystroke="alt z" />
  </action>
</keymap>