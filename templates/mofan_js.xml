<templateSet group="mofan_js">
  <template name="sc" value="&lt;script&gt;&#10;    $END$&#10;&lt;/script&gt;" description="js" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="JSP" value="true" />
    </context>
  </template>
  <template name="scr" value="&lt;script src=&quot;$END$&quot;&gt;&lt;/script&gt;" description="jsr" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="JSP" value="true" />
    </context>
  </template>
  <template name="fan" value="function ($ONE$) {&#10;    $END$&#10;}" description="fan" toReformat="false" toShortenFQNames="true">
    <variable name="ONE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="fn" value="function $NAME$($TWO$) {&#10;    $END$&#10;}" description="fn" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TWO" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>